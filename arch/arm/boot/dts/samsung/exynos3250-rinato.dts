/dts-v1/;
#include "exynos3250.dtsi"
#include "exynos4412-ppmu-common.dtsi"
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/clock/samsung,s2mps11.h>

/ {
	model = "Samsung Rinato board";
	compatible = "samsung,rinato", "samsung,rinato-rev06",
			"samsung,exynos3250", "samsung,exynos3";
	chassis-type = "watch";

	aliases {
		i2c0 = &i2c_pmic;
		i2c1 = &i2c_touchscreen;
		i2c7 = &i2c_max77836;
		i2c14 = &i2c_fuelgauge;
		mmc0 = &mshc_0;
		mmc1 = &mshc_1;
	};

	chosen {
		stdout-path = &serial_1;
	};

	/* For ramoops, recoverable via upload mode */
	reserved-memory {
        #address-cells = <1>;
        #size-cells = <1>;
        ranges;

        ramoops@51000000 {
            compatible = "ramoops";
            reg = <0x51000000 0x100000>;
            record-size = <0x4000>;
            console-size = <0x4000>;
            ftrace-size = <0x4000>;
            pmsg-size = <0x4000>;
            ecc-size = <0x4000>;
			max-reason = <5>; /* Currently KMSG_DUMP_MAX */
        };
	};



	/*
	 * Memory actually starts at 0x40000000,
	 * but retail devices can use the first 16K of RAM for S-Boot debug purposes.
	 * Therefore, make main RAM start a bit later so we can also use this feature.
	 *
	 * Just leaving them in the memory node would result in multiple mappings
	 * and unpredictable behaviour when attempting access in the S-Boot
	 * debug driver.
	 *
	 * Unfortunately, just reserving the memory range doesn't seem to work.
	 */
	memory@40004000 {
		device_type = "memory";
		reg = <0x40004000 0x1fefc000>;
	};

	firmware@205f000 {
		compatible = "samsung,secure-firmware";
		reg = <0x0205f000 0x1000>;
	};

	gpio-keys {
		compatible = "gpio-keys";

		power-key {
			gpios = <&gpx2 7 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_POWER>;
			label = "power key";
			debounce-interval = <10>;
			wakeup-source;
		};
	};

	wlan_pwrseq: mshc1-pwrseq {
		compatible = "mmc-pwrseq-simple";
		reset-gpios = <&gpe0 4 GPIO_ACTIVE_LOW>;
	};

	i2c_max77836: i2c-gpio-0 {
		compatible = "i2c-gpio";
		sda-gpios = <&gpd0 2 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpd0 3 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		#address-cells = <1>;
		#size-cells = <0>;

		max77836: pmic@25 {
			compatible = "maxim,max77836";
			interrupt-parent = <&gpx1>;
			interrupts = <5 IRQ_TYPE_NONE>;
			reg = <0x25>;
			wakeup-source;

			extcon {
				compatible = "maxim,max77836-muic";
			};

			regulators {
				compatible = "maxim,max77836-regulator";
				safeout_reg: SAFEOUT {
					regulator-name = "SAFEOUT";
				};

				charger_reg: CHARGER {
					regulator-name = "CHARGER";
					regulator-min-microamp = <45000>;
					regulator-max-microamp = <475000>;
					regulator-boot-on;
				};

				motor_reg: LDO1 {
					regulator-name = "MOT_2.7V";
					regulator-min-microvolt = <1100000>;
					regulator-max-microvolt = <2700000>;
				};

				LDO2 {
					regulator-name = "UNUSED_LDO2";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <3950000>;
				};
			};

			charger {
				compatible = "maxim,max77836-charger";

				maxim,constant-uvolt = <4350000>;
				maxim,fast-charge-uamp = <225000>;
				maxim,eoc-uamp = <7500>;
				maxim,ovp-uvolt = <6500000>;
			};
		};
	};

	i2c_fuelgauge: i2c-gpio-1 {
		compatible = "i2c-gpio";
		sda-gpios = <&gpd1 2 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpd1 3 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

	/* fails to enumerate with max77836-battery: Failed to locate of_node [id: -1], maybe wrong addr? */
	/* Downstream code suggests true address might be 0xE (14), though 0x36 is also used */
		fuelgauge@36 {
			compatible = "maxim,max77836-battery";
			interrupt-parent = <&gpx1>;
			interrupts = <2 IRQ_TYPE_EDGE_RISING>;
			reg = <0x36>;
		};
	};

	/* For whatever reason, the builtin I2C master doesn't work at all */
	i2c_pmic: i2c-gpio-2 {
		compatible = "i2c-gpio";
		sda-gpios = <&gpd1 0 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpd1 1 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		/* TODO: Unknown peripheral on this bus at addr 0x06 */

		pmic@66 {
			status = "okay";
			compatible = "samsung,s2mps14-pmic";
			interrupt-parent = <&gpx0>;
			interrupts = <7 IRQ_TYPE_LEVEL_LOW>;
			reg = <0x66>;
			wakeup-source;

			s2mps14_osc: clocks {
				compatible = "samsung,s2mps14-clk";
				#clock-cells = <1>;
				clock-output-names = "s2mps14_ap", "unused",
					"s2mps14_bt";
				status = "okay";
			};

			regulators {
				// This is unused in downstream, but having it on at low voltage *probably* doesn't hurt, nothing in dtree uses it
				ldo1_reg: LDO1 {
					status = "okay";
					regulator-name = "VAP_ALIVE_1.0V";
					regulator-min-microvolt = <1000000>;
					regulator-max-microvolt = <1000000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-on-in-suspend;
					};
				};

				// Is vdd_ldo2_range dowstream (1.2 volts)
				ldo2_reg: LDO2 {
					status = "okay";
					regulator-name = "VAP_M1_1.2V";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo3_reg: LDO3 {
					status = "okay";
					regulator-name = "VCC_AP_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Is vddq_mmc range downstream, delivers 1.8V
				ldo4_reg: LDO4 {
					status = "okay";
					regulator-name = "VAP_AVDD_PLL1";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Is vdd_pll_1v8 downstream, delivers 1.0V
				ldo5_reg: LDO5 {
					status = "okay";
					regulator-name = "VAP_PLL_ISO_1.0V";
					regulator-min-microvolt = <1000000>;
					regulator-max-microvolt = <1000000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Is vap_mipi_1.0v downstream
				ldo6_reg: LDO6 {
					status = "okay";
					regulator-name = "VAP_VMIPI_1.0V";
					regulator-min-microvolt = <1000000>;
					regulator-max-microvolt = <1000000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Is vdd_ldo7 range downstream, delivers 1.8V
				ldo7_reg: LDO7 {
					status = "okay";
					regulator-name = "VAP_AVDD_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo8_reg: LDO8 {
					status = "okay";
					regulator-name = "VAP_USB_3.0V";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3000000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo9_reg: LDO9 {
					status = "okay";
					regulator-name = "V_LPDDR_1.2V";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-on-in-suspend;
					};
				};

				// Matches downstream
				ldo10_reg: LDO10 {
					status = "okay";
					regulator-name = "UNUSED_LDO10";
					regulator-min-microvolt = <1000000>;
					regulator-max-microvolt = <1000000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Unused downstream
				ldo11_reg: LDO11 {
					status = "okay";
					regulator-name = "V_EMMC_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					samsung,ext-control-gpios = <&gpk0 2 GPIO_ACTIVE_HIGH>;
				};

				// Unused downstream
				ldo12_reg: LDO12 {
					status = "okay";
					regulator-name = "V_EMMC_2.8V";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;
					samsung,ext-control-gpios = <&gpk0 2 GPIO_ACTIVE_HIGH>;
				};

				// Matches downstream
				ldo13_reg: LDO13 {
					status = "okay";
					regulator-name = "CAM_AVDD_2.8V";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Is regulator-haptic downstream, delivers 1.1V to 2.7V, but seems unused on production revision
				ldo14_reg: LDO14 {
					status = "okay";
					regulator-name = "UNUSED_LDO14";
					regulator-min-microvolt = <2700000>;
					regulator-max-microvolt = <2700000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo15_reg: LDO15 {
					status = "okay";
					regulator-name = "TSP_AVDD_3.3V";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;
					regulator-always-on;
					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Delivers only 3.0V downstream
				ldo16_reg: LDO16 {
					status = "okay";
					regulator-name = "LCD_VDD_3.3V";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo17_reg: LDO17 {
					status = "okay";
					regulator-name = "V_IRLED_3.3V";
					regulator-min-microvolt = <3300000>;
					regulator-max-microvolt = <3300000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo18_reg: LDO18 {
					status = "okay";
					regulator-name = "CAM_AF_2.8V";
					regulator-min-microvolt = <2800000>;
					regulator-max-microvolt = <2800000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo19_reg: LDO19 {
					status = "okay";
					regulator-name = "TSP_VDD_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo20_reg: LDO20 {
					status = "okay";
					regulator-name = "LCD_VDD_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo21_reg: LDO21 {
					status = "okay";
					regulator-name = "CAM_IO_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo22_reg: LDO22 {
					status = "okay";
					regulator-name = "CAM_DVDD_1.2V";
					regulator-min-microvolt = <1200000>;
					regulator-max-microvolt = <1200000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo23_reg: LDO23 {
					status = "okay";
					regulator-name = "HRM_VCC_1.8V";
					regulator-min-microvolt = <1800000>;
					regulator-max-microvolt = <1800000>;
					regulator-always-on;
				};

				// Delivers 3.3V downstream
				ldo24_reg: LDO24 {
					status = "okay";
					regulator-name = "HRM_VCC_3.3V";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3000000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Matches downstream
				ldo25_reg: LDO25 {
					status = "okay";
					regulator-name = "UNUSED_LDO25";
					regulator-min-microvolt = <3000000>;
					regulator-max-microvolt = <3000000>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// vdd_mif range downstream, delivers 0.6 to 1.6V, seems like nothing is connected downstream
				buck1_reg: BUCK1 {
					status = "okay";
					regulator-name = "VAP_MIF_1.0V";
					regulator-min-microvolt = <800000>;
					regulator-max-microvolt = <900000>;
					regulator-always-on;
					// Set by downstream
					regulator-ramp-delay = <12>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Delivers 0.6V to 1.6V downstream
				buck2_reg: BUCK2 {
					status = "okay";
					regulator-name = "VAP_ARM_1.0V";
					regulator-min-microvolt = <850000>;
					regulator-max-microvolt = <1150000>;
					regulator-always-on;
					regulator-ramp-delay = <12>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Delivers 0.6V to 1.6V downstream
				buck3_reg: BUCK3 {
					status = "okay";
					regulator-name = "VAP_INT3D_1.0V";
					regulator-min-microvolt = <850000>;
					regulator-max-microvolt = <1000000>;
					regulator-always-on;
					regulator-ramp-delay = <12>;

					regulator-state-mem {
						regulator-off-in-suspend;
					};
				};

				// Unused downstream
				buck4_reg: BUCK4 {
					status = "okay";
					regulator-name = "VCC_SUB_1.95V";
					regulator-min-microvolt = <1950000>;
					regulator-max-microvolt = <1950000>;
					regulator-always-on;
					regulator-ramp-delay = <12>;

					regulator-state-mem {
						regulator-on-in-suspend;
					};
				};

				// Unused downstream
				buck5_reg: BUCK5 {
					status = "okay";
					regulator-name = "VCC_SUB_1.35V";
					regulator-min-microvolt = <1350000>;
					regulator-max-microvolt = <1350000>;
					regulator-always-on;
					regulator-ramp-delay = <12>;

					regulator-state-mem {
						regulator-on-in-suspend;
					};
				};
			};
		};
	};

	/* For whatever reason, the builtin I2C master doesn't work at all */
	i2c_touchscreen: i2c-gpio-3 {
		compatible = "i2c-gpio";
		sda-gpios = <&gpa0 6 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		scl-gpios = <&gpa0 7 (GPIO_ACTIVE_HIGH | GPIO_OPEN_DRAIN)>;
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		/*
		* Some sources online suggest that the chip is an MCS8000, which would be supported by the MIP4 driver.
		* That's most likely incorrect, as the downstream kernel's driver code doesn't contain basic commands
		* from the MIP4 protocol, and it generally looks much more like an extended MMS152 protocol.
		* Samsung calls the model "Melfas W" in all their source code, no idea what the real model number is.
		*/
		touchscreen@48 {
			compatible = "melfas,mms152";
			reg = <0x48>;
			interrupt-parent = <&gpx3>;
			interrupts = <5 IRQ_TYPE_EDGE_FALLING>;
			status = "okay";

			/* Parameters are likely part of a configuration blob loaded by the driver and are ignored. Here for documentation purposes only. */
			touchscreen-size-x = <320>;
			touchscreen-size-y = <320>;

			avdd-supply = <&ldo15_reg>;
			vdd-supply = <&ldo19_reg>;
		};
	};

	haptics {
		compatible = "regulator-haptic";
		haptic-supply = <&motor_reg>;
		min-microvolt = <1100000>;
		max-microvolt = <2700000>;
	};

	thermal-zones {
		cpu_thermal: cpu-thermal {
			cooling-maps {
				map0 {
					/* Corresponds to 500MHz */
					cooling-device = <&cpu0 5 5>,
							 <&cpu1 5 5>;
				};
				map1 {
					/* Corresponds to 200MHz */
					cooling-device = <&cpu0 8 8>,
							 <&cpu1 8 8>;
				};
			};
		};
	};
};

&adc {
	vdd-supply = <&ldo3_reg>;
	status = "okay";
	assigned-clocks = <&cmu CLK_SCLK_TSADC>;
	assigned-clock-rates = <6000000>;

	thermistor-ap {
		compatible = "murata,ncp15wb473";
		pullup-uv = <1800000>;
		pullup-ohm = <100000>;
		pulldown-ohm = <100000>;
		io-channels = <&adc 0>;
	};

	thermistor-battery {
		compatible = "murata,ncp15wb473";
		pullup-uv = <1800000>;
		pullup-ohm = <100000>;
		pulldown-ohm = <100000>;
		io-channels = <&adc 1>;
	};
};

&bus_dmc {
	devfreq-events = <&ppmu_dmc0_3>, <&ppmu_dmc1_3>;
	vdd-supply = <&buck1_reg>;
	status = "okay";
};

&bus_leftbus {
	devfreq-events = <&ppmu_leftbus_3>, <&ppmu_rightbus_3>;
	vdd-supply = <&buck3_reg>;
	status = "okay";
};

&bus_rightbus {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_lcd0 {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_fsys {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_mcuisp {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_isp {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_peril {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&bus_mfc {
	devfreq = <&bus_leftbus>;
	status = "okay";
};

&cmu {
	clocks = <&xusbxti>;
};

&cpu0 {
	cpu0-supply = <&buck2_reg>;
};

&exynos_usbphy {
	status = "okay";
	vbus-supply = <&safeout_reg>;
};

&hsotg {
	vusb_d-supply = <&ldo15_reg>;
	vusb_a-supply = <&ldo12_reg>;
	dr_mode = "peripheral";
	status = "okay";
};

&dsi_0 {
	vddcore-supply = <&ldo6_reg>;
	vddio-supply = <&ldo6_reg>;
	samsung,burst-clock-frequency = <250000000>;
	samsung,esc-clock-frequency = <20000000>;
	samsung,pll-clock-frequency = <24000000>;
	status = "okay";

	panel@0 {
		compatible = "samsung,s6e63j0x03";
		reg = <0>;
		vdd3-supply = <&ldo16_reg>;
		vci-supply = <&ldo20_reg>;
		reset-gpios = <&gpe0 1 GPIO_ACTIVE_LOW>;
		te-gpios = <&gpx0 6 GPIO_ACTIVE_HIGH>;
	};
};

&fimd {
	status = "okay";

	i80-if-timings {
		cs-setup = <0>;
		wr-setup = <0>;
		wr-active = <1>;
		wr-hold = <0>;
	};
};

&gpu {
	mali-supply = <&buck3_reg>;
	status = "okay";
};





&i2c_1 {
	#address-cells = <1>;
	#size-cells = <0>;
	samsung,i2c-sda-delay = <100>;
	samsung,i2c-slave-addr = <0x10>;
	samsung,i2c-max-bus-freq = <400000>;
	status = "okay";

};

&i2s2 {
	status = "okay";
};

&jpeg {
	status = "okay";
};

&mshc_0 {
	broken-cd;
	non-removable;
	cap-mmc-highspeed;
	mmc-hs200-1_8v;
	card-detect-delay = <200>;
	/* In mainline, LDO12 is set as the supplier, but downstream uses LDO4.
	 * Maybe this is a copy-paste mistake from monk or artik5?
	 * Either, way, it seems like voltage negotiation is broken in a similar way
	 * to one of artik5's MMC controllers, meaning that just leaving vmmc-supply undefined
	 * is apparently the only way to get it to work.
	 * vmmc-supply = <&ldo12_reg>;
	 * vmmc-supply = <&ldo4_reg>;
	 */
	clock-frequency = <100000000>;
	max-frequency = <100000000>;
	mmc-ddr-1_8v;
	samsung,dw-mshc-ciu-div = <1>;
	samsung,dw-mshc-sdr-timing = <0 1>;
	samsung,dw-mshc-ddr-timing = <1 2>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd0_cmd &sd0_bus1 &sd0_bus4 &sd0_bus8>;
	bus-width = <8>;
	status = "okay";
};

&mshc_1 {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;

	non-removable;
	cap-sd-highspeed;
	cap-sdio-irq;
	keep-power-in-suspend;
	samsung,dw-mshc-ciu-div = <1>;
	samsung,dw-mshc-sdr-timing = <0 1>;
	samsung,dw-mshc-ddr-timing = <1 2>;
	pinctrl-names = "default";
	pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus1 &sd1_bus4>;
	bus-width = <4>;

	mmc-pwrseq = <&wlan_pwrseq>;

	brcmf: wifi@1 {
		compatible = "brcm,bcm4334-fmac", "brcm,bcm4329-fmac";
		reg = <1>;

		interrupt-parent = <&gpx1>;
		interrupts = <1 IRQ_TYPE_LEVEL_HIGH>;
		interrupt-names = "host-wake";
	};
};

&serial_0 {
	assigned-clocks = <&cmu CLK_SCLK_UART0>;
	assigned-clock-rates = <100000000>;
	status = "okay";

	bluetooth {
		compatible = "brcm,bcm4334-bt", "brcm,bcm4330-bt", "brcm,bcm4329-bt";
		max-speed = <3000000>;
		shutdown-gpios = <&gpe0 0 GPIO_ACTIVE_HIGH>;
		device-wakeup-gpios = <&gpx3 1 GPIO_ACTIVE_HIGH>;
		host-wakeup-gpios = <&gpx2 6 GPIO_ACTIVE_HIGH>;
		clocks = <&s2mps14_osc S2MPS11_CLK_BT>;
	};
};

&serial_1 {
	status = "okay";
};

&tmu {
	vtmu-supply = <&ldo7_reg>;
	status = "okay";
};

&rtc {
	clocks = <&cmu CLK_RTC>, <&s2mps14_osc S2MPS11_CLK_AP>;
	clock-names = "rtc", "rtc_src";
	status = "okay";
};

&xusbxti {
	clock-frequency = <24000000>;
};

&spi_0 {
	status = "okay";

	/* Taken from artik5, let's hope they match */
	cs-gpios = <&gpx3 4 GPIO_ACTIVE_LOW>, <0>;
	assigned-clocks = <&cmu CLK_MOUT_SPI0>, <&cmu CLK_DIV_SPI0>,
		<&cmu CLK_DIV_SPI0_PRE>, <&cmu CLK_SCLK_SPI0>;
	assigned-clock-parents = <&cmu CLK_DIV_MPLL_PRE>, /* for: CLK_MOUT_SPI0 */
		<&cmu CLK_MOUT_SPI0>,    /* for: CLK_DIV_SPI0 */
		<&cmu CLK_DIV_SPI0>,     /* for: CLK_DIV_SPI0_PRE */
		<&cmu CLK_DIV_SPI0_PRE>; /* for: CLK_SCLK_SPI0 */

	/* Taken pretty much straight from binding description */
	sensorhub@0 {
		status = "disabled";
		compatible = "samsung,sensorhub-rinato";
		reg = <0>;
		spi-max-frequency = <5000000>;
		interrupt-parent = <&gpx0>;
		interrupts = <2 0>;
		ap-mcu-gpios = <&gpx0 0 GPIO_ACTIVE_HIGH>;
		mcu-ap-gpios = <&gpx0 4 GPIO_ACTIVE_HIGH>;
		mcu-reset-gpios = <&gpx0 5 GPIO_ACTIVE_HIGH>;

		/* Also taken from artik5, not sure if needed */
		controller-data {
			samsung,spi-feedback-delay = <2>;
		};
	};
};



